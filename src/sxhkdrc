super + Return
	~/Software/launch "Terminal" xfce4-terminal

super + d
	~/Software/menurun ~/Software/menu

super + w
	~/Software/launch "Browser" brave-browser

super + e
	~/Software/launch "File Manager" thunar

super + t
	~/Software/thono

super + m
	~/Software/launch "Music Player" ~/Software/music ~/Music

super + q
	bspc node -c

super + {_,shift + }f
	bspc node -t \~{fullscreen,floating}

super + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

super + {_,shift + }{1-9}
	bspc {desktop -f,node -d} {1-9}
