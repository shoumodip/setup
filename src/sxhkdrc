super + Return
	alacritty

super + w
	~/Software/brave/brave-browser

super + e
	thunar

super + u
	slock

super + t
	~/Software/thono

super + s
	~/Software/thono -s

super + shift + s
	maim -s $(date '+%s').png

super + d
	~/Software/menurun

super + q
	bspc node -c

super + {_,shift + }f
	bspc node -t \~{fullscreen,floating}

super + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

super + {_,shift + }{1-9}
	bspc {desktop -f,node -d} {1-9}
