{
    "include once": {
        "prefix": "#once",
        "body": [
            "#ifndef ${1:FILE}_H",
            "#define $1_H",
            "",
            "$0",
            "",
            "#endif // $1_H"
        ]
    },
    "ifdef": {
        "prefix": "#ifdef",
        "body": [
            "#ifndef ${1}",
            "$0",
            "#endif // $1"
        ]
    },
    "ifndef": {
        "prefix": "#ifndef",
        "body": [
            "#ifndef ${1}",
            "$0",
            "#endif // $1"
        ]
    }
}
