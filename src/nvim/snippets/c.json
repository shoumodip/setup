{
    "include once": {
        "prefix": "#once",
        "body": [
            "#ifndef ${1:FILE}_H",
            "#define $1_H",
            "$0",
            "",
            "#endif // $1_H"
        ],
        "description": "Header include guard"
    },
    "todo": {
        "body": "assert(0 && \"TODO: not implemented\");",
        "description": "Todo"
    },
    "unreachable": {
        "body": "assert(0 && \"unreachable\");",
        "description": "Unreachable"
    }
}
